<?xml version="1.0" encoding="utf-8"?>
<com.example.rodneytressler.budge.Views.PokeMapView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/lib/com.example.rodneytressler.budget"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <com.google.android.gms.maps.MapView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/mapView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="13dp"
        android:id="@+id/floatingActionButton" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_menu_mapmode"
        android:layout_alignBottom="@+id/floatingActionButton"
        android:layout_centerHorizontal="true"
        android:id="@+id/floatingActionButton2" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/presence_busy"
        android:layout_alignBottom="@+id/floatingActionButton2"
        android:layout_alignParentEnd="true"
        android:id="@+id/floatingActionButton3" />


</com.example.rodneytressler.budge.Views.PokeMapView>